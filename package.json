{
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "clean": "rm -rf dist .tmp",
    "lint": "./node_modules/.bin/eslint app/scripts/**/*.js; exit 0",
    "build-css": "./node_modules/.bin/node-sass app/styles/main.scss dist/styles/main.css",
    "postcss": "postcss dist/styles/main.css --use autoprefixer cssnano -o dist/styles/main.css",
    "html": "cp app/*.html dist/; cp app/*.txt dist/"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "material-design-lite": "^1.3.0",
    "mdl-selectfield": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "browser-sync": "^2.2.1",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "favicons": "^4.8.6",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cache": "^1.0.1",
    "gulp-eslint": "^4.0.0",
    "gulp-favicons": "^2.2.6",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^4.0.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-strip-debug": "^1.0.2",
    "gulp-uglify": "^3.0.0",
    "gulp-uncss": "^1.0.4",
    "gulp-useref": "^3",
    "main-bower-files": "^2.5.0",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.0.0",
    "wiredep": "^4"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true,
      "blockBindings": true,
      "arrowFunctions": true
    },
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "false": {}
}
